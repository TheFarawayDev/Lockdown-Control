{
  "manifest_version": 3,
  "name": "Lockdown Control",
  "version": "1.1.3",
  "description": "Lockdown Chrome to certain tabs, mostly for kids who are in Detention.",
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  }
}  